Algoritmo ejercicio_11
    //Transforma de dólares a pesos
	
    //var
    Definir dolares, pesos Como Real;
	
    //Solicitar datos
    Escribir "Digita la cantidad de dólares que tienes:";
    Leer dolares;
	
    pesos <- dolares * 11.96; //Esperemos que no baje más el peso
	
    Escribir "La cantidad en sheintavos fue: $", pesos;
FinAlgoritmo
