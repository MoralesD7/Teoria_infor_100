Algoritmo ejercicio_09
    //Realiza una calculadora
	
    //var
    Definir a, b, suma, resta, multi, div Como Entero;
	
    //Soli
    Escribir "Digita los datos a, b:";
    Leer a;
    Leer b;
	
    //op
    suma <- a + b;
    resta <- a - b;
    multi <- a * b;
    div <- a / b;
	
    Escribir "El resultado de las operaciones es:", "\nsuma:", suma, "\nresta:", resta, "\nmultiplicación:", multi, "\ndivisión:", div;
FinAlgoritmo
