Algoritmo ejercicio_12
    //Problema de acres y conversiones
	
    //var
    Definir acres, metros, hectarea Como Real;
	
    //Solicita
    Escribir "Digita la cantidad de acres:";
    Leer acres;
	
    metros <- acres * 4047;
    hectarea <- metros * 10000;
	
    Escribir "La cantidad de hectáreas fue: ", hectarea, " metros cuadrados";
FinAlgoritmo
